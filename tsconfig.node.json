{
  "compilerOptions": {
    "composite": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "lib": [
      "ES2015",
      "ES2015.Collection",
      "ES2015.Iterable",
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,
  "allowSyntheticDefaultImports": true,
  "forceConsistentCasingInFileNames": true,
  "strict": true
  },
  "include": ["vite.config.ts"]
}
